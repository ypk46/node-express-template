{
  "name": "ninja-news-api",
  "version": "0.1.0",
  "description": "Template for a Node.js API with TypeScript using Express.js",
  "main": "index.js",
  "type": "module",
  "repository": "https://github.com/ypk46/node-express-template.git",
  "scripts": {
    "tsc": "tsc",
    "start": "pm2-runtime start dist/index.js",
